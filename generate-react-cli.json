{
    "usesTypeScript": true,
    "usesCssModule": true,
    "testLibrary": "Testing Library",
    "cssPreprocessor": "css",
    "component": {
        "default": {
            "path": "components",
            "withLazy": false,
            "withStory": false,
            "withStyle": true,
            "withTest": true,
            "customTemplates": {
                "component": "templates/components/TemplateName.tsx",
                "lazy": "templates/components/TemplateName.lazy.js",
                "style": "templates/components/TemplateName.style.css",
                "test": "templates/components/TemplateName.test.js"
            }
        },
        "page": {
            "path": "pages",
            "withLazy": false,
            "withStory": false,
            "withStyle": true,
            "withTest": true,
            "customTemplates": {
                "component": "templates/pages/TemplateName.tsx",
                "lazy": "templates/pages/TemplateName.lazy.js",
                "style": "templates/pages/TemplateName.style.css",
                "test": "templates/pages/TemplateName.test.js"
            }
        }
    }
}